$('#content<%= @answer.id %>').html('<div class="btn-group" id="content"><%= link_to like_question_answer_path(@answer.question, @answer), method: :put, remote: true do %><i class="fa fa-thumbs-o-up fa-sm"></i>(<%= @answer.get_upvotes.size %>)<% end %> <%= link_to unlike_question_answer_path(@answer.question, @answer), method: :put,remote: true do %><i class="fa fa-thumbs-o-down fa-sm"></i>(<%= @answer.get_downvotes.size %>)<% end %></div>');