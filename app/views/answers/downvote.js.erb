$('#content<%= @answer.id %>').html('<div class="btn-group" id="content"><%= link_to like_question_answer_path(@answer.question, @answer), method: :put, remote: true, class: "btn btn-success btn-xs" do %>Like(<%= @answer.get_upvotes.size %>)<% end %><%= link_to unlike_question_answer_path(@answer.question, @answer), method: :put,remote: true, class: "btn btn-danger btn-xs" do %>Dislike(<%= @answer.get_downvotes.size %>)<% end %></div>');
